#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/学习资料/源码/zx-design/node_modules/.pnpm/vitepress@1.6.3_@algolia+cl_01c5dc43da2e4fd2a43f92947afbcf77/node_modules/vitepress/bin/node_modules:/mnt/d/学习资料/源码/zx-design/node_modules/.pnpm/vitepress@1.6.3_@algolia+cl_01c5dc43da2e4fd2a43f92947afbcf77/node_modules/vitepress/node_modules:/mnt/d/学习资料/源码/zx-design/node_modules/.pnpm/vitepress@1.6.3_@algolia+cl_01c5dc43da2e4fd2a43f92947afbcf77/node_modules:/mnt/d/学习资料/源码/zx-design/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/学习资料/源码/zx-design/node_modules/.pnpm/vitepress@1.6.3_@algolia+cl_01c5dc43da2e4fd2a43f92947afbcf77/node_modules/vitepress/bin/node_modules:/mnt/d/学习资料/源码/zx-design/node_modules/.pnpm/vitepress@1.6.3_@algolia+cl_01c5dc43da2e4fd2a43f92947afbcf77/node_modules/vitepress/node_modules:/mnt/d/学习资料/源码/zx-design/node_modules/.pnpm/vitepress@1.6.3_@algolia+cl_01c5dc43da2e4fd2a43f92947afbcf77/node_modules:/mnt/d/学习资料/源码/zx-design/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
